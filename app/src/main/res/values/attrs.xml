<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableTextViewAttrs">
        <attr name="collapsed_line" format="integer" />
        <attr name="isCollapsed" format="boolean" />
        <attr name="expand_text_color" format="color"/>
        <attr name="collpased_text_color" format="color"/>
        <attr name="expand_text" format="string" />
        <attr name="collpased_text" format="string" />
    </declare-styleable>

    <declare-styleable name="TabLayoutAttrs">
        <attr name="item_horizontal_margin" format="integer" />
        <attr name="item_vertical_margin" format="integer" />
    </declare-styleable>
</resources>
